      </search>
    </input>
    <input type="dropdown" token="description" searchWhenChanged="true">
      <label>Description</label>
      <choice value="*">All</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>description</fieldForLabel>
